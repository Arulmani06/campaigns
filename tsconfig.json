{
    "compilerOptions": {
      "target": "ES2015",
      "module": "commonjs",
      "sourceMap": true,
      "removeComments": true,
      "strict": false,
      "noImplicitAny": true,
      "strictNullChecks": true,
      "noImplicitThis": true,
      "noUnusedLocals": true,
      "noUnusedParameters": true,
      "noImplicitReturns": true,
      "esModuleInterop": true
    }
  }